**.node[*].netwType = "WiseRoute"
**.node[*].net.trace = true
**.node[*].net.stats = true
**.node[*].net.useSimTracer = false
**.node[*].net.headerLength = 24 bit
**.node[*].net.sinkAddress = 0
# RSSI threshold for route selection
**.node[*].net.rssiThreshold = -50 dBm
# If set to zero, this node does not initiates route tree building.
# If set to a value larger than zero, this nodes periodically initiates route tree building.
**.node[*].net.routeFloodsInterval = 100 s